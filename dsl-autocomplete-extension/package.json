{
  "name": "dsl-autocomplete-extension",
  "displayName": "Projeto DSL Autocomplete",
  "description": "Extension for autocomplete and syntax highlighting of Projeto DSL language.",
  "version": "0.1.0",
  "publisher": "projeto-dsl",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onLanguage:dsl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "dsl",
        "aliases": ["Projeto DSL", "DSL"],
        "extensions": [".dsl"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dsl",
        "scopeName": "source.dsl",
        "path": "./syntaxes/dsl.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "dsl",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/node": "^14.x",
    "typescript": "^4.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "ts-loader": "^8.0.0"
  },
  "dependencies": {}
}